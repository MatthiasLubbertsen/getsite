# GitHub Personal Access Token met repo permissies
GITHUB_TOKEN=your_github_personal_access_token_here

# GitHub gebruiker/organisatie naam
GITHUB_OWNER=your_github_username

# Naam van de PRIVATE repo waar pagina's worden opgeslagen
GITHUB_PAGES_REPO=my-pages-storage

# Branch naam (meestal main of master)
GITHUB_BRANCH=main

# Optioneel: API key voor extra beveiliging
API_SECRET=your_random_secret_key